<script>
	var myButton = document.getElementById("myButton");
		
	
	function display2ndBut(){
		setTimeout(function(){myButton.style.display=""}, 10000)
	}
	function displayDate() 
	{
	console.log("hello")
  	myButton.value = new Date();
	}
	
   

</script>
